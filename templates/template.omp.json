{{=<% %>=}}
{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "blocks": [
    {
      "alignment": "left",
      "newline": true,
      "segments": [
        {
          "foreground": "#<%colors.accent.hex%>",
          "properties": {
            "time_format": "15:04"
          },
          "style": "plain",
          "template": "[{{ .CurrentDate | date .Format }}]",
          "type": "time"
        },
        {
          "foreground": "#<%colors.sky.hex%>",
          "style": "plain",
          "template": " {{ if .WSL }}WSL at {{ end }}{{.Icon}} {{ .UserName }}@{{ .HostName }} ",
          "type": "os"
        },
        {
          "foreground": "#<%colors.flamingo.hex%>",
          "foreground_templates": [
            "{{ if or (.Working.Changed) (.Staging.Changed) }}#<%colors.rosewater.hex%>{{ end }}",
            "{{ if and (gt .Ahead 0) (gt .Behind 0) }}#<%colors.peach.hex%>{{ end }}",
            "{{ if gt .Ahead 0 }}#<%colors.mauve.hex%>{{ end }}",
            "{{ if gt .Behind 0 }}#<%colors.mauve.hex%>{{ end }}"
          ],
          "properties": {
            "branch_template": "{{ trunc 25 .Branch }}",
            "fetch_stash_count": true,
            "fetch_status": true,
            "fetch_upstream_icon": true
          },
          "style": "plain",
          "template": " {{ .UpstreamIcon }}{{ .HEAD }}{{if .BranchStatus }} {{ .BranchStatus }}{{ end }}{{ if .Working.Changed }} \uf044 {{ .Working.String }}{{ end }}{{ if and (.Working.Changed) (.Staging.Changed) }} |{{ end }}{{ if .Staging.Changed }} \uf046 {{ .Staging.String }}{{ end }}{{ if gt .StashCount 0 }} \ueb4b {{ .StashCount }}{{ end }} ",
          "type": "git"
        }
      ],
      "type": "prompt"
    },
    {
      "alignment": "right",
      "segments": [
        {
          "type": "status",
          "style": "plain",
          "foreground": "#<%colors.accent.hex%>",
          "foreground_templates": [
            "{{ if gt .Code 0 }}#<%colors.red.hex%>{{ end }}"
          ],
          "template": " x{{ reason .Code }}"
        },
        {
          "foreground": "#<%colors.accent.hex%>",
          "foreground_templates": [
            "{{ if gt .Code 0 }}#<%colors.red.hex%>{{ end }}"
          ],
          "properties": {
            "style": "roundrock",
            "always_enabled": true
          },
          "style": "diamond",
          "template": " {{ .FormattedMs }} ",
          "type": "executiontime"
        }
      ],
      "type": "prompt"
    },
    {
      "alignment": "left",
      "newline": true,
      "segments": [
        {
          "foreground": "#<%colors.accent.hex%>",
          "properties": {
            "style": "full"
          },
          "style": "plain",
          "template": "\ue5ff {{ .Path }}",
          "type": "path"
        }
      ],
      "type": "prompt"
    },
    {
      "alignment": "left",
      "newline": true,
      "segments": [
        {
          "foreground": "#<%colors.accent.hex%>",
          "style": "plain",
          "template": "\u0021",
          "type": "root"
        },
        {
          "foreground": "#<%colors.accent.hex%>",
          "style": "plain",
          "template": "\u276f",
          "type": "text"
        }
      ],
      "type": "prompt"
    }
  ],
  "final_space": true,
  "version": 3
}
